@model IEnumerable<Raymax.Models.Terms>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TermsANDPolicy)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.TermsANDPolicy)
                </td>
            </tr>
        }
    </tbody>
</table>
